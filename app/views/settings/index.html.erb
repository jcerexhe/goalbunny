<h1>Listing Settings</h1>

<table class="table table">
  <thead>
    <tr>
      <th>Mailing address</th>
      <th>Suburb</th>
      <th>State</th>
      <th>Postcode</th>
      <th>Country</th>
      <th>Goal cancellation refund time delay (in days)</th>
      <th>Share completed goals?</th>
      <th>Dream</th>
      <th>Age</th>
      <th>Occupation</th>
      <th>Sex</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.mailing_address %></td>
        <td><%= setting.suburb %></td>
        <td><%= setting.state %></td>
        <td><%= setting.postcode %></td>
        <td><%= setting.country %></td>
        <td><%= setting.goal_cancellation_refund_time_delay_in_days %></td>
        <td><%= setting.share_completed_goals? %></td>
        <td><%= setting.dream %></td>
        <td><%= setting.age %></td>
        <td><%= setting.occupation %></td>
        <td><%= setting.sex %></td>
        <td><%= link_to 'Show', setting %></td>
        <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting', new_setting_path %>
